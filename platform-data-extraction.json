    "endpoints": [
      {
        "path": "/api/team-data",
        "method": "GET",
        "authentication_required": false,
        "description": "Returns monthly team data with averages and individual employee metrics for all months",
        "response_structure": {
          "months": [
            {
              "month": "number (8-11)",
              "averages": "team average metrics",
              "employees": "array of employee data for this month"
            }
          ]
        }
      },
      {
        "path": "/api/metrics",
        "method": "GET",
        "authentication_required": false,
        "description": "Returns current team metrics and calculated values",
        "response_structure": {
          "wellbeingIndex": "number",
          "depressionLevel": "number",
          "anxietyLevel": "number",
          "burnoutIndex": "number",
          "sleepDuration": "number",
          "sleepQuality": "number",
          "workLifeBalance": "number",
          "stressLevel": "number",
          "atRiskCount": "number",
          "criticalCount": "number"
        }
      },
      {
        "path": "/api/employee-data",
        "method": "GET",
        "authentication_required": false,
        "description": "Returns complete employee data with history, alerts, recommendations, and metadata",
        "response_structure": {
          "company": "string",
          "lastUpdated": "date",
          "employees": "array",
          "teamAverages": "object",
          "industryBenchmarks": "object",
          "alerts": "array",
          "recommendations": "array",
          "metadata": "object"
        }
      }
    ]
  },
  "metrics_definitions": {
    "who5": {
      "name": "WHO-5 Well-Being Index",
      "range": "0-100",
      "description": "World Health Organization Well-Being Index measuring quality of life and well-being",
      "thresholds": {
        "critical": "< 28 (probable depression)",
        "at_risk": "< 50 (risk of reduced well-being)",
        "healthy": ">= 50"
      },
      "interpretation": "Higher is better"
    },
    "phq9": {
      "name": "PHQ-9 Patient Health Questionnaire",
      "range": "0-27",
      "description": "Depression screening and severity measurement",
      "thresholds": {
        "minimal": "0-4",
        "mild": "5-9",
        "moderate": "10-14",
        "moderately_severe": "15-19",
        "severe": "20-27",
        "at_risk": "> 10",
        "critical": "> 15"
      },
      "interpretation": "Lower is better"
    },
    "gad7": {
      "name": "GAD-7 Generalized Anxiety Disorder",
      "range": "0-21",
      "description": "Anxiety screening and severity measurement",
      "thresholds": {
        "minimal": "0-4",
        "mild": "5-9",
        "moderate": "10-14",
        "severe": "15-21",
        "at_risk": "> 10",
        "critical": "> 15"
      },
      "interpretation": "Lower is better"
    },
    "mbi": {
      "name": "MBI Maslach Burnout Inventory",
      "range": "0-100%",
      "description": "Burnout assessment measuring emotional exhaustion and depersonalization",
      "thresholds": {
        "low": "< 30%",
        "moderate": "30-40%",
        "high": "> 40%",
        "at_risk": "> 35%"
      },
      "interpretation": "Lower is better"
    },
    "sleepDuration": {
      "name": "Sleep Duration",
      "range": "0-24 hours",
      "description": "Average nightly sleep duration",
      "thresholds": {
        "insufficient": "< 6 hours",
        "adequate": "6-8 hours",
        "optimal": "7-9 hours"
      },
      "interpretation": "7-9 hours is optimal"
    },
    "sleepQuality": {
      "name": "Sleep Quality",
      "range": "0-10",
      "description": "Self-reported sleep quality rating",
      "thresholds": {
        "poor": "< 5",
        "fair": "5-6",
        "good": "7-8",
        "excellent": "9-10"
      },
      "interpretation": "Higher is better"
    },
    "workLifeBalance": {
      "name": "Work-Life Balance",
      "range": "0-10",
      "description": "Self-reported work-life balance satisfaction",
      "thresholds": {
        "poor": "< 5",
        "fair": "5-6",
        "good": "7-8",
        "excellent": "9-10"
      },
      "interpretation": "Higher is better"
    },
    "stressLevel": {
      "name": "Stress Level",
      "range": "0-40 (PSS scale)",
      "description": "Perceived Stress Scale measurement",
      "thresholds": {
        "low": "0-13",
        "moderate": "14-26",
        "high": "27-40"
      },
      "interpretation": "Lower is better"
    }
  },
  "risk_calculation": {
    "at_risk_criteria": "Any of: WHO-5 < 50, PHQ-9 > 10, GAD-7 > 10, MBI > 35",
    "critical_criteria": "Any of: WHO-5 < 28, PHQ-9 > 15, GAD-7 > 15"
